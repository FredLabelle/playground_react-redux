{
  "name": "investorx-frontend",
  "private": false,
  "version": "0.0.1",
  "description": "InvestorX frontend",
  "main": "server.js",
  "repository": "https://github.com/eFounders/investorx",
  "author": "eFounders",
  "license": "UNLICENSED",
  "scripts": {
    "eslint": "eslint .;exit 0",
    "prettier": "prettier --print-width 100 --single-quote --trailing-comma all --write \"*.js\" \"{actions,components,lib,pages,reducers}/**/*.js\"",
    "dev": "NODE_ENV='development' node server.js",
    "build": "next build",
    "start": "node server.js",
    "deploy-staging": "now -E .env/staging.env && now alias investorx-staging-frontend.efounders.co",
    "deploy": "now -E .env/production.env && now alias investorx-frontend.efounders.co",
    "logs-staging": "now logs investorx-staging-frontend.efounders.co",
    "logs": "now logs investorx-frontend.efounders.co",
    "cleanup": "now rm investorx-frontend --safe -y"
  },
  "dependencies": {
    "autoprefixer": "^7.1.2",
    "babel-plugin-transform-replace-object-assign": "^0.2.1",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "babel-preset-airbnb": "^2.4.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "graphql-tag": "^2.4.2",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "next": "^2.4.7",
    "postcss-easy-import": "^2.1.0",
    "postcss-loader": "^2.0.6",
    "prop-types": "^15.5.10",
    "raw-loader": "^0.5.1",
    "react": "^15.6.1",
    "react-addons-shallow-compare": "^15.6.0",
    "react-apollo": "^1.4.3",
    "react-cookie": "^2.0.8",
    "react-country-region-selector": "^1.1.0",
    "react-dates": "^12.2.4",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-redux-toastr": "^7.0.0",
    "react-sortable-hoc": "^0.6.5",
    "redux": "^3.7.2",
    "semantic-ui-react": "^0.71.0",
    "universal-cookie-express": "^2.0.6",
    "uploadcare-widget": "^3.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.0.1",
    "file-loader": "^0.11.2",
    "prettier": "^1.5.3"
  }
}
