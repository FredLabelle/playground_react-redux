{
  "name": "investorx-backend",
  "private": false,
  "version": "0.0.1",
  "description": "InvestorX backend",
  "main": "server.js",
  "repository": "https://github.com/eFounders/investorx",
  "author": "eFounders",
  "license": "UNLICENSED",
  "scripts": {
    "setup-dev": "npm install --only=dev",
    "eslint": "eslint .;exit 0",
    "prettier": "prettier --print-width 100 --single-quote --trailing-comma es5 --write \"*.js\" \"{forest,graphql,lib,models,routes,services}/**/*.js\"",
    "dev": "NODE_ENV='development' nodemon",
    "deploy-staging": "now -E .env/staging.env && now alias investorx-staging-backend.efounders.co",
    "deploy": "now -E .env/production.env && now alias investorx-backend.efounders.co",
    "logs-staging": "now logs investorx-staging-backend.efounders.co",
    "logs": "now logs investorx-backend.efounders.co",
    "cleanup": "now rm investorx-backend --safe -y"
  },
  "dependencies": {
    "@google-cloud/storage": "^1.1.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "cors": "^2.8.3",
    "dataloader": "^1.3.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "forest-express-sequelize": "^1.1.7",
    "googleapis": "^19.0.0",
    "graphql": "^0.10.3",
    "graphql-date": "^1.0.3",
    "graphql-server-express": "^0.8.5",
    "graphql-tools": "^1.0.0",
    "graphql-type-json": "^0.1.4",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "node-mailjet": "^3.1.0",
    "pg": "^6.4.0",
    "pg-hstore": "^2.3.2",
    "request": "^2.81.0",
    "sequelize": "^4.2.0",
    "shortid": "^2.2.8",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-config-prettier": "^2.2.0",
    "eslint-plugin-import": "^2.3.0",
    "nodemon": "^1.11.0",
    "prettier": "^1.4.4"
  }
}
